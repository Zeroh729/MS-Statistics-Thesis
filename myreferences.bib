doi = {10.1016/j.patcog.2017.03.009},
file = {:D$\backslash$:/{\~{}}Masters/{\~{}} MS-STAT/{\~{}}THESIS/Papers/Papers - Model Based Clustering/Choy - Fuzzy model-based clustering and its application in image segmentation.pdf:pdf},
issn = {00313203},
journal = {Pattern Recognition},
keywords = {Fuzzy algorithm,Image segmentation},
pages = {141--157},
