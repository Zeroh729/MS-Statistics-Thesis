title = {{A Climate Classification of China through k-Nearest-Neighbor and Sparse Subspace Representation}},
url = {https://journals.ametsoc.org/jcli/article/33/1/243/346140/A-Climate-Classification-of-China-through},
volume = {33},
year = {2020}
}
