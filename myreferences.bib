doi = {10.1371/journal.pone.0228624},
file = {:D$\backslash$:/{\~{}}Masters/{\~{}} MS-STAT/{\~{}}THESIS/Papers/Supplementary Papers/Chen - Identification and quantification of cassava starch adulteration in different food starches by droplet digital PCR.pdf:pdf},
isbn = {1111111111},
