issn = {13652621},
journal = {International Journal of Food Science and Technology},
keywords = {Droplet digital PCR,food safety,meat products,species identification},
number = {3},
pages = {1145--1150},
title = {{Application of the novel Droplet digital PCR technology for identification of meat species}},
volume = {55},
year = {2020}
}
